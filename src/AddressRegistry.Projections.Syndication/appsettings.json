{
  "ConnectionStrings": {
    "SyndicationProjections": "Server=(localdb)\\mssqllocaldb;Database=EFProviders.InMemory.AddressRegistry;Trusted_Connection=True;",
    "SyndicationProjectionsAdmin": "Server=(localdb)\\mssqllocaldb;Database=EFProviders.InMemory.AddressRegistry;Trusted_Connection=True;"
  },

  "DataDog": {
    "ServiceName": "address-registry-syndication-projections"
  },

  "SyndicationFeeds": {
    "Municipality": "https://legacy.gemeente.staging-basisregisters.vlaanderen/v1/gemeenten/sync?embed=true",
    "MunicipalityAuthUserName": "x",
    "MunicipalityAuthPassword": "x",
    "MunicipalityPollingInMilliseconds": 5000,
    "StreetName": "https://legacy.streetname.staging-basisregisters.vlaanderen/v1/straatnamen/sync?embed=true",
    "StreetNameAuthUserName": "x",
    "StreetNameAuthPassword": "x",
    "StreetNamePollingInMilliseconds": 5000
  },

  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning",
      "AddressRegistry": "Information",
      "Aiv": "Warning",
      "System": "Warning",
      "Microsoft": "Warning"
    }
  },

  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "AddressRegistry": "Debug",
        "Aiv": "Warning",
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Properties": {
      "Application": "AddressRegistry - Syndication Projections"
    }
  }
}
